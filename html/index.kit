                    </div>
                    <div class="company text-center col-2 p-1">
                        <img class="img-fluid mx-auto" src="../dist/img/firmy/Mapei.jpg" alt="logo bolix">
                    </div>
                    <div class="company text-center col-2 p-1">
                        <img class="img-fluid mx-auto" src="../dist/img/firmy/sabo.jpg" alt="logo bolix">
                    </div>
                    <div class="company text-center col-2 p-1">
                        <img class="img-fluid mx-auto" src="../dist/img/firmy/ceresit.jpg" alt="logo bolix">
                    </div>
                </div>
            </div>
        </section>


        <!-- Kontakt  -->
        <section class="contact wrapper py-5" id="contact">
            <h2 class="section-title">Kontakt</h2>
            <div class="underline"></div>
            <p><i class="fas fa-phone"></i> +48 517 512 426</p>
            <p><i class="fas fa-at"></i> mail@mail.pl</p>

        </section>


    </main>

